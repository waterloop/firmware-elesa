{
  "name": "firmware-elesa",
  "version": "1.0.0",
  "description": "G6 Dashboard",
  "main": "Frontend/main.js",
  "scripts": {
    "start": "concurrently --kill-others \"electron-forge start\" \"npm run watch\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "watch": "webpack --config ./Frontend/webpack.common.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waterloop/firmware-elesa.git"
  },
  "author": "Waterloop",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/waterloop/firmware-elesa/issues"
  },
  "homepage": "https://github.com/waterloop/firmware-elesa#readme",
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@electron-forge/cli": "^6.0.0-beta.67",
    "@electron-forge/maker-deb": "^6.0.0-beta.67",
    "@electron-forge/maker-rpm": "^6.0.0-beta.67",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.67",
    "@electron-forge/maker-zip": "^6.0.0-beta.67",
    "@svgr/webpack": "^6.5.1",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.5.0",
    "css-loader": "^6.7.1",
    "electron": "^21.1.0",
    "electron-reload": "^2.0.0-alpha.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.4.3",
    "styled-components": "^5.3.6",
    "styled-react-modal": "^3.0.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "firmware_elesa"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
